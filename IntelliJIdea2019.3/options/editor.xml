    <option name="COLLAPSE_SEQUENTIAL_COMMENTS" value="true" />
  </component>
 <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}GOOD.*$">
      <option name="FOREGROUND" value="547854" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}BAD.*$">
      <option name="FOREGROUND" value="943e3e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\*  )[^\n]*$">
      <option name="FOREGROUND" value="498aca" />
      <option name="BACKGROUND" value="191919" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\*{2}  )[^\n]*$">
      <option name="FOREGROUND" value="498aca" />
      <option name="BACKGROUND" value="111111" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="498aca" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\*{3}  )[^\n]*$">
      <option name="FOREGROUND" value="56a6f2" />
      <option name="BACKGROUND" value="0" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="56a6f2" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <pattern useCustomColors="true" pattern="\s{1}fail\b$">
      <option name="FOREGROUND" value="943e3e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\s{1}pass\b$">
      <option name="FOREGROUND" value="547854" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=///// )[^\n]*$">
      <option name="FOREGROUND" value="afafaf" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="afafaf" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=////)[^\n]*$">
      <option name="FOREGROUND" value="9b9b9b" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
    <pattern useCustomColors="true" pattern="\s{1}error\b$">
      <option name="FOREGROUND" value="943e3e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=! )[^\n]*$">
      <option name="FOREGROUND" value="943e3e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=///)[^\n]*$">
      <option name="FOREGROUND" value="9b9b9b" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=##)[^\n]*$">
      <option name="FOREGROUND" value="9b9b9b" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=###)[^\n]*$">
      <option name="FOREGROUND" value="9b9b9b" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=#### )[^\n]*$">
      <option name="FOREGROUND" value="afafaf" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="afafaf" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>
